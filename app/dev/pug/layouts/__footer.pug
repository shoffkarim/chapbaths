<script type="text/javascript">
  (function(){
    document.onreadystatechange = () => {
      if (document.readyState === 'complete') {
        let el = document.querySelector('#logolines');
        let myAnimation = new LazyLinePainter(el, {"ease":"easeInOutCubic","strokeWidth":15.2,"strokeOpacity":1,"strokeColor":"#292323","reverse":true,"repeat":100});
        myAnimation.paint();
      }
    }
  })();
</script>
